{
  "name": "nuvix",
  "homepage": "https://nuvix.io/support",
  "description": "Nuvix is an backend server that abstract and simplify complex and repetitive development tasks behind a very simple REST API",
  "version": "0.0.1-alpha.2",
  "license": "BSD-3-Clause",
  "main": "dist/cjs/sdk.js",
  "exports": {
    ".": {
      "import": "./dist/esm/sdk.js",
      "require": "./dist/cjs/sdk.js",
      "types": "./types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/esm/sdk.js",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/nuvix-tech/sdk-for-web"
  },
  "scripts": {
    "build": "npm run build:types && npm run build:libs",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir types",
    "build:libs": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.2",
    "playwright": "1.15.0",
    "rollup": "2.75.4",
    "serve-handler": "6.1.0",
    "tslib": "2.4.0",
    "typescript": "4.7.2"
  },
  "files": [
    "dist",
    "types"
  ],
  "jsdelivr": "dist/iife/sdk.js",
  "unpkg": "dist/iife/sdk.js",
  "dependencies": {
    "socket.io-client": "^4.8.1"
  }
}
